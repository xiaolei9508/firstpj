<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cmft.slas.cmuop.mapper.CmuopPointInfoTypeMapper">
    <select id="getMaxInfoType" resultType="java.lang.String">
        SELECT
        MAX(CAST(info_type AS UNSIGNED))
        FROM t_cmuop_point_info_type
        WHERE is_delete = 0
    </select>
</mapper>